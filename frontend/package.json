{
  "name": "mun-website",
  "version": "1.0.0",
  "description": "Brown MUN Website",
  "author": "Ben Navetta",
  "private": true,
  "scripts": {
    "dll:dev": "NODE_ENV=development webpack --config tooling/webpack/vendor.config.babel.js",
    "dll:prod": "NODE_ENV=production webpack --config tooling/webpack/vendor.config.babel.js",
    "build:dev": "NODE_ENV=development webpack --config tooling/webpack/webpack.config.babel.js",
    "build:prod": "NODE_ENV=production webpack --config tooling/webpack/webpack.config.babel.js",
    "start:prod": "NODE_ENV=production webpack-dashboard -- babel-node tooling/server.js",
    "start:dev": "NODE_ENV=development webpack-dashboard -- babel-node tooling/server.js",
    "start": "yarn run start:dev",
    "build": "yarn run build:prod"
  },
